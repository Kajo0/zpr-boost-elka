<?xml version="1.0" encoding="UTF-8"?>
<!-- szablon konfiguracji mapy miasta -->

<map>
	<nodes>
		<node x="1.0" y="1.0">1</node>	<!-- x, y, id wierzcholka -->
		<node x="1.0" y="2.0">2</node>
		<node x="2.0" y="2.0">3</node>
		<node x="2.0" y="1.0">4</node>
	</nodes>

	<edges>						<!-- krawedzie - graf nieskierowany -->
		<edge from="1" to="2" />		<!-- krawedz pomiedzy wierzcholkami (from):(to) -->
		<edge from="2" to="3" />
		<edge from="3" to="4" />
	</edges>
</map>